<%= simple_form_for(@boarding_booking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :patient %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :confirmed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
